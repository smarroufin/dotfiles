{
  "log1": {
    "prefix": [
      "log1"
    ],
    "body": [
      "console.log($1)"
    ]
  },
  "log2": {
    "prefix": [
      "log2"
    ],
    "body": [
      "console.log('$1', $1)"
    ]
  },
  "logDeep": {
    "prefix": [
      "logDeep"
    ],
    "body": [
      "console.dir($1, { depth: null })"
    ]
  },
  "sleep": {
    "prefix": [
      "sleep"
    ],
    "body": [
      "await new Promise(function(resolve) { setTimeout(resolve, $1) })"
    ]
  },
  "unique": {
    "prefix": [
      "unique"
    ],
    "body": [
      "[...new Set($1)]"
    ]
  }
}
